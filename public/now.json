{
    "version": 2,
    "name": "techbook",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/img/(.*)",
            "dest": "/img/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}